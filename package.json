{
  "name": "umi-ts",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-umi-ts",
    "stop": "egg-scripts stop --title=egg-server-umi-ts",
    "dev": "egg-bin dev",
    "build": "npm run umi:build",
    "umi:dev": "umi dev",
    "umi:build": "APP_ROOT=$PWD/app/web UMI_ENV=prod CI=true umi build",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint_egg.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "antd": "^3.16.1",
    "classnames": "^2.2.6",
    "egg": "^2.6.1",
    "egg-scripts": "^2.6.0",
    "egg-view-assets": "^1.4.4",
    "egg-view-nunjucks": "^2.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "umi": "^2.6.11"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "tslint-react-hooks": "^2.0.0",
    "typescript": "^3.0.0",
    "umi-plugin-react": "^1.7.2",
    "umi-types": "^0.3.2"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
